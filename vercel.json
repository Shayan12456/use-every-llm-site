{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "nextjs",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 10,
      "memory": 512
    }
  },
  "regions": ["iad1"],
  "public": false
}
